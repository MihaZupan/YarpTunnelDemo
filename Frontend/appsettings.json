{
  //"X-Tunnel-Token": "foo",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "alpha",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "alpha": {
        "Destinations": {
          "alpha/destination1": {
            "Address": "http://local-tunnel/"
          }
        }
      }
    }
  },
  "LettuceEncrypt": {
    "AcceptTermsOfService": true,
    "DomainNames": [ "pvt-eu.mihubot.xyz" ],
    "EmailAddress": "lets-encrypt@mihubot.xyz"
  }
}
